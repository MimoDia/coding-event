var now = new Date();

(function($, Drupal, drupalSettings){



   $(document).ready(function(){
   
    dateDuchamps = new Date(drupalSettings.participer.participer.dateDuchamps);

          if(dateDuchamps < now){
           $('.addtocalendar').remove();
	  }

   });

})(jQuery, Drupal, drupalSettings);


